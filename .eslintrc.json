{
  "extends": [
    "next/core-web-vitals",
    "@rocketseat/eslint-config/next",
    "plugin:cypress/recommended"
  ],
  "plugins": [
    "simple-import-sort",
    "cypress"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "no-console": "warn"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "globals": {
    "cy": true
  }
}